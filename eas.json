{
  "cli": {
    "version": ">= 14.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "EXPO_ANDROID_GRADLE_CACHE_DISABLED": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
